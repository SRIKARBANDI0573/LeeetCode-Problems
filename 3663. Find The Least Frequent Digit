class Solution {
    public int getLeastFrequentDigit(int n) {
        int a[]=new int[10];
        while(n!=0){
            int r=n%10;
            a[r]++;
            n=n/10;
        }
        int max=0;
        int min=Integer.MAX_VALUE;

        for(int i=0;i<10;i++){
            if(a[i]<min && a[i]!=0){
                min=a[i];
                max=i;
            }
        }
        return max;
    }
}
