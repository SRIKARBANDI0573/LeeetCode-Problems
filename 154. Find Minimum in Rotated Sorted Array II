class Solution {
    public int findMin(int[] nums) {
        // Arrays.sort(nums);
        // return nums[0];
        int n=nums.length;
        int l=0;
        int h=n-1;
        while(l<h){
            int mid=l+(h-l)/2;

            if(nums[mid]>nums[h]){
                l=mid+1;
            }
            else if(nums[mid]<nums[h]){
                h=mid;
            }
            else{
                h--;
            }
        }
        return nums[l];
    }
}
