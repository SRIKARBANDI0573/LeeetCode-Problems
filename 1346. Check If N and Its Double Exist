class Solution {
    public boolean checkIfExist(int[] arr) {
         int c=0;
         int n=arr.length;
        HashSet<Integer> hs = new HashSet<>();
        // for(int i = 0; i < n; i++){
        //     hs.add(arr[i]);
        // }
        for(int x:arr){
            if(hs.contains(2*x)){
                return true;
            }
            if(x%2 == 0 && hs.contains(x/2)){
                return true;
            }
            hs.add(x);
    }
    return false;
}
}
