class Solution {
    public int reverseDegree(String s) {
         int d = 0;  
        
        for (int i = 0; i < s.length(); i++) {
            char a = s.charAt(i);
            int b = 26 - (a - 'a');  
            int x = i + 1; 
            d += b * x;
        }
        
        return d;
    }

    }
