class Solution {
    public int sumOfUnique(int[] nums) {
        int n = nums.length;
        Arrays.sort(nums);
        int sum = 0;
        HashMap<Integer,Integer> hm = new HashMap<>();

        for(int x:nums){
             hm.put(x,hm.getOrDefault(x,0)+1);
        }
        for(int keys: hm.keySet()){
            if(hm.get(keys) == 1){
                sum+=keys;
            }
        }
        return sum;
    }
}
