class Solution {
    public boolean searchMatrix(int[][] matrix, int target) {
        int i=matrix.length;
        int j=matrix[0].length;

        int c=0;
        for(int x=0;x<i;x++){
            for(int y=0;y<j;y++){
                if(matrix[x][y]==target){
                    c=1;
                }
            }
        }
        if(c==1){
            return true;
        }
        else{
            return false;
        }
    }
}
