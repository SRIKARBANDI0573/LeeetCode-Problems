class Solution {
    public int totalNumbers(int[] digits) {
         boolean[] used = new boolean[1000]; 
        int count = 0;
        int n = digits.length;

        for (int i = 0; i < n; i++) {   
            if (digits[i] == 0) continue;

            for (int j = 0; j < n; j++) {  
                if (j == i) continue; 

                for (int k = 0; k < n; k++) {  
                    if (k == i || k == j) continue;
                    if (digits[k] % 2 != 0) continue;

                    int num = digits[i] * 100 + digits[j] * 10 + digits[k];

                    if (!used[num]) {  
                        used[num] = true;
                        count++;
                    }
                }
            }
        }
        return count;
    }
    }Â©leetcode
